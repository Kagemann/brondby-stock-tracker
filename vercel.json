{
  "version": 2,
  "builds": [
    {
      "src": "dashboard.py",
      "use": "@vercel/python"
    },
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
