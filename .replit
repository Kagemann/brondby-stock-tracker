run = "python main.py"
entrypoint = "main.py"

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "${REPL_HOME}"

[packager]
language = "python3"

[packager.features]
packageSearch = true
guessImports = true

[languages]
[languages.python3]
pattern = "**/*.py"
syntax = "python"

[languages.python3.languageServer]
start = [ "pylsp" ]
